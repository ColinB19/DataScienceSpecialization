pollutantmean <- function(directory, pollutant, id = 1:332){
num = 0
dem = 0
for (i in id){
if(id[[i]] < 10){
temp <- paste('00', id[[i]], sep='')
}else if (id[[i]] >+ 10 && id[[i]]< 100){
temp <- paste('0', id[[i]], sep='')
}else{
temp <- paste(id[[i]], sep='')
}
file_path <- paste(directory,"/", temp, '.csv', sep = '')
temp <- read.csv(file_path)
pol <- temp[pollutant]
pol_no_na = pol[!is.na(pol)]
num <- num + sum(pol_no_na)
dem <- dem + sum(is.na(pol))
}
num/dem
}
